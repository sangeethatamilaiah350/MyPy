def search(self,matrix, n, m, x): 
    	
    	k=m-1# checking the last number since the row is in ascending order
    	for i in range(n):
    	    
    	    if matrix[i][k]>=x:
    	        
    	        for j in range(m-1,-1,-1):#checking from the last row
    	            
    	            if x==matrix[i][j]:
    	                return True
    	    
    	    
    	    
    	return False	
